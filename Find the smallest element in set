// Find the smallest element in an array
#include <bits/stdc++.h>
#include <algorithm>
#include <vector>
#include<iostream>
#include <set>
using namespace std;


int main(){
    int n;
    cin>>n;
    int arr[n] ;
    for (int i=0;i<n;i++){
        cin>>arr[i];
    }
    set <int> s;
 for (int i=0;i<n;i++){        s.insert(arr[i]);
    }
        cout<< *s.begin()<<"\t";

    

    
    return 0;
}
